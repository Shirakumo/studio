<header>
  <h1>Studio</h1>
  <nav>
    <a href="#" @href="studio/"><i class="fas fa-home"></i> Home</a>
    <c:unless test="(auth:current)">
      <a href="#" @href="<auth page login #>"><i class="fas fa-sign-in-alt"></i> Login</a>
    </c:unless>
